@model IEnumerable<Jarvis.Models.SMSDemand>

@{
    ViewBag.Title = "SMS Demand List";
}

<h2>SMS Demand List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SMSCode.ReferenceCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SMSCode.CodeDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content_tr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content_en)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SMSCode.ReferenceCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SMSCode.CodeDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Content_tr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Content_en)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
            @Html.ActionLink("Details", "Details", new { id = item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ID })
        </td>
    </tr>
}

</table>
